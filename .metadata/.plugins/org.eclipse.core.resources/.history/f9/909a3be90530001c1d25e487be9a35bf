server:
  port: 2027
spring:
  datasource:
    url: 'jdbc:mysql://localhost:3306/MyDatabase'
    username: root
    password: 123N@ndini04
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: 'true'
    properties:
      hibernate:
        format_sql: 'true'
        dialect: org.hibernate.dialect.MySQL5Dialect


